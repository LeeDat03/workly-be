		"@types/jsonwebtoken": "^9.0.10",
		"@types/morgan": "^1.9.10",
		"@types/node": "^24.9.1",
		"@types/nodemailer": "^7.0.3",
		"@typescript-eslint/eslint-plugin": "^8.46.2",
		"@typescript-eslint/parser": "^8.46.2",
		"eslint": "^9.38.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.4",
		"nodemon": "^3.1.10",
		"prettier": "^3.6.2",
		"rimraf": "^6.0.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.9.3"
	}
