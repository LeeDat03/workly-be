	"version": "1.0.0",
	"description": "User-Company Service Backend with Node.js, Express, TypeScript, and Neo4j",
	"main": "dist/server.js",
	"scripts": {
		"dev": "nodemon",
		"build": "tsc",
		"start": "node dist/server.js",
		"start:dev": "ts-node src/server.ts",
		"lint": "eslint src/**/*.ts",
		"lint:fix": "eslint src/**/*.ts --fix",
		"format": "prettier --write \"src/**/*.ts\"",
		"format:check": "prettier --check \"src/**/*.ts\"",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [
		"nodejs",
		"express",
		"typescript",
		"neo4j",
		"microservice"
	],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"bcryptjs": "^3.0.2",
		"compression": "^1.8.1",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"dotenv": "^17.2.3",
		"express": "^5.1.0",
		"helmet": "^8.1.0",
		"jsonwebtoken": "^9.0.2",
		"morgan": "^1.10.1",
		"neo4j-driver": "^6.0.0",
		"neogma": "^1.15.1",
		"nodemailer": "^7.0.10",
		"uuid": "^13.0.0",
		"winston": "^3.16.0",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.6",
